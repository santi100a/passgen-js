{
  "name": "@santi100/passgen",
  "version": "1.0.6",
  "repository": {
    "url": "https://github.com/santi100a/passgen-js.git"
  },
  "description": "A random password generator written in JavaScript.",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "ts": "tsc",
    "build": "tsc && pkg . --out-path dist/ --compress GZip",
    "build:debug": "tsc && pkg . --debug --out-path dist/ --compress GZip"
  },
  "pkg": {
    "assets": [
      "node_modules/figlet/fonts/*.*",
      "package.json"
    ],
    "options": [
      "experimental-modules"
    ]
  },
  "keywords": [],
  "author": "@santi100",
  "license": "Other",
  "type": "module",
  "dependencies": {
    "@santi100/assertion-lib": "^1.0.3",
    "@santi100/coloring-lib": "^1.0.5",
    "@santi100/random-lib": "^1.0.5",
    "commander": "^9.4.1",
    "figlet": "^1.5.2",
    "inquirer": "^9.1.4"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.11",
    "pkg": "^5.8.0"
  }
}
